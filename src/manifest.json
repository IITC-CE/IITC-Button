{
  "name": "IITC Button",
  "description": "__MSG_extDescription__",
  "minimum_chrome_version": "61",
  "applications": {
    "gecko": {
      "id": "iitc@modos189.ru",
      "strict_min_version": "57.0"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "https://intel.ingress.com/*",
    "https://missions.ingress.com/*",
    "*://*/*.json",
    "*://*/*.meta.js",
    "*://*/*.user.js",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage"
  ],
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches" : ["https://intel.ingress.com/*", "https://missions.ingress.com/*"],
      "run_at": "document_start",
      "js": ["js/content-script.js"]
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
    "default_title": "__MSG_titleDefault__",
    "default_icon": {
      "48": "assets/icons/48/icon.png",
      "96": "assets/icons/96/icon.png"
    }
  },
  "icons": {
    "48": "assets/icons/48/icon.png",
    "128": "assets/icons/128/icon.png"
  },
  "manifest_version": 2
}
